{
    "name": "haxis.me-website",
    "version": "0.0.0",
    "scripts": {
        "watch": "npm-watch build",
        "start": "webpack serve",
        "build": "webpack",
        "stats": "webpack --env production generateprofile --profile --json > stats.json"
    },
    "watch": {
        "build": {
            "patterns": [
                "src",
                "test"
            ],
            "extensions": "ts,html,css",
            "quiet": false
        }
    },
    "dependencies": {
        "@lucsoft/webgen": "^1.17.18",
        "@webpack-cli/serve": "^1.3.0",
        "css-loader": "^5.2.0",
        "css-minimizer-webpack-plugin": "^1.3.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.1",
        "mini-css-extract-plugin": "^1.3.9",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.18",
        "typescript": "^4.2.3",
        "webpack": "^5.28.0",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.5.0"
    },
    "devDependencies": {
        "copy-webpack-plugin": "^8.1.1",
        "terser-webpack-plugin": "^5.1.1",
        "webpack-dev-server": "^3.11.2"
    }
}
